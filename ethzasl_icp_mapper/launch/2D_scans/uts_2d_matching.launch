<launch>

  <param name="/use_sim_time" value="true"/>

	<node name="uts_2d_matching" type="uts_2d_matching" pkg="ethzasl_icp_mapper" output="screen" >
	<param name="icpConfig" value="$(find ethzasl_icp_mapper)/launch/2D_scans/icp_loop.yaml" />
	<param name="inputFiltersConfig" value="$(find ethzasl_icp_mapper)/launch/2D_scans/input_filters.yaml" />
    
  </node>

<node pkg="rviz" type="rviz" name="rviz" args="-d $(find ethzasl_icp_mapper)/launch/2D_scans/matching.rviz" output="screen"/>
</launch>
